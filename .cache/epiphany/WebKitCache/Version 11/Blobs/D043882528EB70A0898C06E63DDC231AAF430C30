window.evergageBeforeSiteConfigExecution = function() {
    try {
// Contact Forms
var captureAndTrackOnNextPage = function (actionName) {
    var data = {};
    if (ajq("[name='email']").length > 0 && ajq("[name='email']").val().length > 0) {
        data.userEmail = ajq("[name='email']").val();
    }
    if (ajq("[name='product']").length > 0 && ajq("[name='product']").val().length > 0) {
        data.productAdviceProduct = ajq("[name='product']").val();
    }
    if (ajq("a.select2-choice span.select2-chosen").first().text().length > 0) {
        data.howWeCanHelp = ajq("a.select2-choice span.select2-chosen").first().text();
    }
    if (ajq("[name='reporter_status']:checked").length > 0) {
        data.reporterStatus = ajq("[name='reporter_status']:checked").val();
    }
    if (ajq("[name='first_name']").length > 0 && ajq("[name='first_name']").val().length > 0) {
        data.firstName = ajq("[name='first_name']").val();
    }
    if (ajq("[name='last_name']").length > 0 && ajq("[name='last_name']").val().length > 0) {
        data.lastName = ajq("[name='last_name']").val();
    }
    if (data.userEmail && /^.+@.+\..+$/.test(data.userEmail)) {
        Evergage.setUser(data.userEmail);
        Evergage.sendActionEventOnNextPage(actionName, data, 5000);
    }
};

var seg_xid = ajq.cookie("seg_xid");
if (seg_xid) {
    Evergage.setCustomField("seg_xid", seg_xid, Evergage.Scope.Request);
}

var bindContactFormListener = function (submitSelector, action) {
    Evergage.safeClick(submitSelector, function () {
        captureAndTrackOnNextPage(action);
    });
};

// Purchasing Licensing Help
if (/^\/?company\/contact\/purchasing\-licensing$/.test(window.location.pathname)) {
    bindContactFormListener("form[name='ca-billing-licensing'] input[value='Next']", "Pricing/Billing/Licensing Help Form Submission");
}

// Product Advice Form Submission
if (/^\/?company\/contact\/product\-evaluator\-advice$/.test(window.location.pathname)) {
    bindContactFormListener("form[name='pa-evaluation-inquiry'] input[value='Next']", "Product Advice Form Submission");
    Evergage.safeClick("div.solvvy-auto-modal", function (e) {
        if (ajq(e.target).closest(".submission-intent-view button, #solvvy-modal i.modal-close").length > 0) {
            captureAndTrackOnNextPage("Product Advice Form Submission");
        }
    });
}

// General Inquiries
if (/^\/?company\/contact\/general\-inquiries$/.test(window.location.pathname)) {
    bindContactFormListener("form[name='ca-general-inquiries'] .button.js-cst-form-submit", "General Inquiry Form Submission");
}

// Contact CEOs
if (/^\/?company\/contact\/contact\-ceos$/.test(window.location.pathname)) {
    bindContactFormListener("form[name='ceo-contact-our-ceos'] #jq_form_submit", "Feedback for the Founders Form Submission");
}

// Blogs - Subscribe Now
Evergage.safeSubmit("#gform_1", function () {
    var blogSubscriptionType = ajq("#gform_1 input[type='checkbox']:checked").val();
    var email = ajq("#gform_1 [name='input_3']").val();
    if (email && /^.+@.+\..+$/.test(email) && blogSubscriptionType) {
        Evergage.setUser(email);
        Evergage.setCustomField("userEmail", email, Evergage.Scope.Request);
        Evergage.setCustomField("blogSubscriptionType", blogSubscriptionType, Evergage.Scope.Request);
        Evergage.trackAction("Blog Subscription Form Submission");
    }
});

// JIRA Sign Up For More Articles
Evergage.safeSubmit("form.exact-target-form__form-element", function () {
    var email = ajq(this).find("[name='email']").val();
    if (email && /^.+@.+\..+$/.test(email)) {
        Evergage.util.runWhenReady(function () {
            return ajq(".exact-target-form__success-message:visible").length > 0; // Success
        }, function () {
            if (window.location.hostname === "bitbucket.org") {
                Evergage.setUser(email);
                Evergage.setCustomField("userEmail", email, Evergage.Scope.Request);
                Evergage.trackAction("Bitbucket - Git Subscription Submission");
            } else {
                Evergage.setUser(email);
                Evergage.setCustomField("userEmail", email, Evergage.Scope.Request);
                Evergage.trackAction("JIRA Sign Up For More Articles Submission");
            }
        }, 200, 15);
    }
});

// Git News Sign Ups
Evergage.safeClick("form.newsletter", function (e) {
    var thisForm = ajq(this);
    if (ajq(e.target).closest("[type='submit'].icon-email-submit").not(".icon-email-error").length > 0) {
        var email = ajq(this).find("input[type='email']").val();
        if (email && /^.+@.+\..+$/.test(email)) {
            Evergage.util.runWhenReady(function () {
                return thisForm.find("[type='submit'].icon-email-success").length > 0; // Success
            }, function () {
                Evergage.setUser(email);
                Evergage.setCustomField("userEmail", email, Evergage.Scope.Request);
                Evergage.trackAction("Git News Sign Up Submission");
            }, 200, 15);
        }
    }
});

// CS-3710 - Bitbucket Signups 
Evergage.safeSubmit("form[action='/account/signup/']", function () {
    var data = {};
    if (ajq(this).find("[name='email']")) {
        data.userEmail = ajq.trim(ajq(this).find("[name='email']").val());
    }
    if (ajq(this).find("[name='full_name']")) {
        data.fullName = ajq.trim(ajq(this).find("[name='full_name']").val());
    }
    if (data.userEmail && /^.+@.+\..+$/.test(data.userEmail) && data.fullName) {
        Evergage.setUser(data.userEmail);
        Evergage.sendActionEventOnNextPage("Bitbucket - Signup Form Submission", data, 6000);
    }
});

// CS-3757 - Send data to Optimizely
Evergage.addExperienceListener(function(experienceId, statType, message) {
    window.optimizely = window.optimizely || [];
    if (message.campaignState === "Testing" || message.campaignState === "Disabled") {
        return;
    }
    var statTypeWord = "";
    if (statType === "i") {
        statTypeWord = "Impression";
    }
    if (statType === "c") {
        statTypeWord = "Clickthrough";
    }
    if (statType === "d") {
        statTypeWord = "Dismissal";
    }
    var optimizelyEventName = [message.campaign, message.experienceName + 
        (message.userGroup === "Control" ? " Control" : ""), statTypeWord].join("-");
    window.optimizely.push(["trackEvent", optimizelyEventName]);
});

Evergage.meta = (function() {
    "use strict";

    var productFamilyDict = {
        "bamboo": "bamboo",
        "bitbucket": "bitbucket",
        "confluence": "confluence",
        "crowd": "crowd",
        "crucible": "crucible",
        "fisheye": "fisheye",
        "hipchat": "hipchat",
        "service": "service-desk",
        "question": "questions",
        "source": "sourcetree",
        "status": "statuspage",
        "stride": "stride",
        "calendar": "team-calendars",
        "trello": "trello",
        "core": "core",
        "portfolio": "portfolio",
        "jira": "jira"
    }

    var deploymentDict = {
        "self-hosted": "server",
        "server": "server",
        "cloud": "cloud",
        "enterprise": "data-center"
    }

    return {
        validateAndTrimItem: function(item, noId) {

            if (typeof item !== "object") return false;

            for (var k in item) {
                if ((item.hasOwnProperty(k)) && (typeof item[k] == "string")) {
                    item[k] = ajq.trim(item[k]);
                    if (item[k] === "") {
                        Evergage.sendException(new Error("Empty string for item attribute: " + k), "Site-wide JavaScript");
                        item[k] = null;
                    }
                }
                if (k == "url" || k == "imageUrl") {
                    item[k] = Evergage.util.qualifyUrl(item[k]);
                }
            }

            if (typeof item._id == "number") {
                item._id = item._id.toString();
            }

            if (!item._id && !noId) {
                Evergage.sendException(new Error("Invalid item id."), "Site-wide JavaScript");
                return false;
            }

            if (item.type == Evergage.ItemType.Tag) {
                if (!(item.tagType == "Brand" ||
                        item.tagType == "Gender" ||
                        item.tagType == "ItemClass" ||
                        item.tagType == "Style" ||
                        item.tagType == "Author" ||
                        item.tagType == "Keyword" ||
                        item.tagType == "ContentClass")) {
                    Evergage.sendException(new Error("Invalid tag type."), "Site-wide JavaScript");
                    return false;
                }
            } else if (!(item.type == Evergage.ItemType.Product ||
                    item.type == Evergage.ItemType.Category ||
                    item.type == Evergage.ItemType.Article ||
                    item.type == Evergage.ItemType.Blog)) {
                Evergage.sendException(new Error("Invalid item type"), "Site-wide JavaScript");
                return false;
            }

            var isValid = true;

            if (item.tags) {
                for (var i = 0; i < item.tags.length; i++) {
                    isValid = isValid && Evergage.meta.validateAndTrimItem(item.tags[i]);
                }
            }

            if (item.categories) {
                for (var j = 0; j < item.categories.length; j++) {
                    isValid = isValid && Evergage.meta.validateAndTrimItem(item.categories[j]);
                }
            }

            return isValid;

        },

        validateLineItem: function(lineItem, isOrder) {
            if (lineItem.sku && !lineItem.sku.id) {
                if (isOrder) {
                    lineItem.sku = null;
                } else {
                    return false;
                }
            }
            var noId = (lineItem.sku && !lineItem.item._id) ? true : false;
            return (Evergage.meta.validateAndTrimItem(lineItem.item, noId) && lineItem.quantity > 0 && typeof(lineItem.item.price) == "number");
        },

        validateOrder: function(order) {
            var isValid = true;
            if (order.lineItems) {
                for (var i = 0; i < order.lineItems.length; i++) {
                    isValid = isValid && Evergage.meta.validateLineItem(order.lineItems[i], true);
                }
            }

            if (typeof order.orderId == "string") {
                order.orderId = ajq.trim(order.orderId);
                if (order.orderId === "") {
                    Evergage.sendException(new Error("Empty string for order Id"), "Site-wide JavaScript");
                    order.orderId = null;
                }
            }

            return isValid;
        },

        getDateFromString: function(dateString) {
            if (!dateString) {
                return;
            }
            var month, day, year;
            var MONTHDICT = {
                "jan":"01","january":"01","feb":"02","february":"02","mar":"03","march":"03","apr":"04","april":"04",
                "may":"05","jun":"06","june":"06","jul":"07","july":"07","aug":"08","august":"08","sep":"09","sept":"09",
                "september":"09","oct":"10","october":"10","nov":"11","november":"11","dec":"12","december":"12"
            };
            month = Evergage.util.extractFirstGroup(/([a-zA-Z]+)/, dateString);
            if (month && MONTHDICT[month.toLowerCase()]) {
                month = MONTHDICT[month.toLowerCase()];
                day = Evergage.util.extractFirstGroup(/[\s-](\d{1,2})[\s,-]/, dateString);
                year = Evergage.util.extractFirstGroup(/(\d{4})/, dateString);
                if (year && month && year) {
                    return year + "-" + month + "-" + day;
                }
            }

        },

        getArticleContent: function(metaTag) {
            var tag = ajq("meta[property='article:" + metaTag + "']:first");

            if (tag.length > 0) {
                return ajq.trim(tag.attr("content"));
            } else {
                return null;
            }
        },

        toTitleCase: function (str) {
            return str.replace(/\w\S*/g, function (txt) {
                return txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase();
            });
        },

        detectArticle: function() {

            var article = {
                type:               Evergage.ItemType.Article,
                _id:                Evergage.util.getLastPathComponentWithoutExtension(window.location.href),
                name:               ajq.trim(Evergage.util.getOpenGraphContent("title").split("- Atlassian")[0].split("| Atlassian")[0]),
                description:        Evergage.util.getOpenGraphContent("description"),
                url:                Evergage.util.getOpenGraphContent("url")
            };

            var imageUrl = Evergage.util.getOpenGraphContent("image");
            if (imageUrl) {
                article.imageUrl = imageUrl;
            }

            var tags = [];
            var productList = [];
            var productElems = ajq(".imkt-product a[href*='/software/']")
            for (var j = 0; j < productElems.length; j++) {
                var productId = Evergage.util.getLastPathComponentWithoutExtension(productElems.eq(j).first().attr("href").split("/try")[0]).replace(/jira-/, "");
                var baseProduct = Evergage.meta.partialMatchProduct(productId);
                if (baseProduct && productList.indexOf(baseProduct) === -1) {
                    var product = {
                        type:               Evergage.ItemType.Tag,
                        tagType:            "Brand",
                        _id:                baseProduct
                    };
                    if (product._id) {
                        productList.push(product._id);
                        tags.push(product);
                    }
                }
            }

            if (ajq(".author-info h4").length > 0) {
                var author = {
                    type:           Evergage.ItemType.Tag,
                    tagType:        "Author",
                    _id:            ajq.trim(ajq(".author-info h4").first().text()),
                    name:           ajq.trim(ajq(".author-info h4").first().text())
                }

                if (author._id) {
                    tags.push(author);
                }
            }

            if (ajq(".topic-title a[href*='topics=']").length > 0) {
                var topic = {
                    type:           Evergage.ItemType.Tag,
                    tagType:        "Keyword",
                    _id:            ajq.trim(ajq(".topic-title a[href*='topics=']").first().text())
                }

                if (topic._id) {
                    tags.push(topic);
                }
            }

            if (ajq(".topic-title a[href*='types=']").length > 0) {
                var contentClass = {
                    type:           Evergage.ItemType.Tag,
                    tagType:        "ContentClass",
                    _id:            ajq.trim(ajq(".topic-title a[href*='types=']").first().text())
                }

                if (contentClass._id) {
                    tags.push(contentClass);
                }
            }

            if (tags.length > 0) {
                article.tags = tags;
            }

            return article;

        },

        detectBaseProduct: function (id) {
            var baseProduct = "";
            for (var key in productFamilyDict) {
                if (productFamilyDict.hasOwnProperty(key) && id.toLowerCase() === productFamilyDict[key]) {
                     baseProduct = productFamilyDict[key];
                     break;
                }
            }

            return baseProduct;
        },

        partialMatchProduct: function (id) {
            var baseProduct = "";
            for (var key in productFamilyDict) {
                var re = new RegExp(key, "g");
                if (productFamilyDict.hasOwnProperty(key) && re.test(id)) {
                     baseProduct = productFamilyDict[key];
                     break;
                }
            }

            return baseProduct;
        },

        detectDeploymentLevel: function (id) {
            var deploymentLevel = "";
            for (var key in deploymentDict) {
                if (deploymentDict.hasOwnProperty(key) && id.toLowerCase() === key) {
                     deploymentLevel = deploymentDict[key].toLowerCase();
                     break;
                }
            }

            return deploymentLevel;
        },

        detectProduct: function() {
            var id = Evergage.util.getLastPathComponentWithoutExtension(ajq(".imkt-navbar__title-link").first().attr("href")) ||
                     Evergage.util.getLastPathComponentWithoutExtension(ajq(".subnavigation--product.imkt-sticky a[href*='/software/'].cms-link--image").first().attr("href")) ||
                     Evergage.util.getLastPathComponentWithoutExtension(ajq(".item-heading .subnav-heading a").first().attr("href"));
            var url  = ajq(".imkt-navbar__title-link").first().prop("href") || ajq(".subnavigation--product.imkt-sticky a[href*='/software/'].cms-link--image").first().prop("href");
            var imageUrl = ((ajq(".imkt-navbar__title-link img") || {})[0] || {}).src|| ajq(".subnavigation--product.imkt-sticky a[href*='/software/'].cms-link--image img").first().attr("src");

            if (!id && !/atlassian.com/.test(window.location.origin) && ajq("body.stride, body.statuspage, body.bitbucket").length > 0) {
               var domainId = (Evergage.util.getOpenGraphContent("site_name") || "").toLowerCase() || (location.host.match(/([^.]+)\.\w{2,3}(?:\.\w{2})?$/) || [])[1];

               if (domainId && domainId.length > 0) {
                   id = domainId;
               }
            }

            var baseProduct = Evergage.meta.detectBaseProduct(id);
            if (baseProduct == "") {
                Evergage.sendException(new Error("Invalid Product ID"), "Site-wide JavaScript");
                return;
            }

            var product = {
                type:               Evergage.ItemType.Product,
                _id:                id,
                price:              1
            };

            return product;
        },

        detectBlog: function() {

            var blog = {
                type:               Evergage.ItemType.Blog,
                _id:                Evergage.util.getLastPathComponentWithoutExtension(window.location.href),
                name:               ajq.trim(ajq("h1#headline").first().text().split("- Atlassian")[0]) || ajq.trim(Evergage.util.getOpenGraphContent("title").split("- Atlassian")[0]),
                description:        Evergage.util.getOpenGraphContent("description").substring(0, 250),
                url:                Evergage.util.getOpenGraphContent("url"),
                imageUrl:           ajq("img.post-main-image, img.default-img").first().attr('src') || Evergage.util.getOpenGraphContent("image"),
                published:          ajq("meta[property='article:published_time']").attr("content")
            };

            if (/^blog.bitbucket.org/.test(window.location.host)) {
                blog.name = ajq.trim(ajq("h1.entryTitle").first().text());
            }

            var tags = [];
            var productList = [];
            var productElems = ajq("main a[href*='/software/']");

            if (productElems.length === 0 && !/atlassian.com/.test(window.location.origin)) {
               var domainId = (Evergage.util.getOpenGraphContent("site_name") || "").toLowerCase();

               if (domainId && domainId.length > 0) {
                   var baseProduct = Evergage.meta.partialMatchProduct(domainId);
                   if (baseProduct && productList.indexOf(baseProduct) === -1) {
                        var product = {
                            type:               Evergage.ItemType.Tag,
                            tagType:            "Brand",
                            _id:                baseProduct
                        };
                        if (product._id) {
                            tags.push(product);
                        }
                    }
                }
            } else {
                for (var j = 0; j < productElems.length; j++) {
                    var productId = Evergage.util.getLastPathComponentWithoutExtension(((productElems.eq(j).first().attr("href") || "").split("/software/")[1] || "").split("/")[0] || "").replace(/jira-/, "");
                    var baseProduct = Evergage.meta.partialMatchProduct(productId);
                    if (baseProduct && productList.indexOf(baseProduct) === -1) {
                        var product = {
                            type:               Evergage.ItemType.Tag,
                            tagType:            "Brand",
                            _id:                baseProduct
                        };
                        if (product._id) {
                            productList.push(product._id);
                            tags.push(product);
                        }
                    }
                }
            }

            var keywordList = [];
            var keywordElems = ajq(".categories a[rel='category tag'], .tags a[rel='tag']")
            for (var j = 0; j < keywordElems.length; j++) {
                var keywordId = Evergage.util.getLastPathComponentWithoutExtension(keywordElems.eq(j).first().attr("href").split("/try")[0]).replace(/jira-/, "");
                if (keywordList.indexOf(keywordId) === -1) {
                    var keyword = {
                        type:               Evergage.ItemType.Tag,
                        tagType:            "Keyword",
                        _id:                keywordId
                    };
                    if (keyword._id) {
                        keywordList.push(keyword._id);
                        tags.push(keyword);
                    }
                }
            }

            if (ajq("span[itemprop*=author] span[itemprop=name]").length > 0) {
                var author = {
                    type:           Evergage.ItemType.Tag,
                    tagType:        "Author",
                    _id:            ajq.trim(ajq("span[itemprop*=author] span[itemprop=name]").first().text()),
                    name:           ajq.trim(ajq("span[itemprop*=author] span[itemprop=name]").first().text())
                }

                if (author._id) {
                    tags.push(author);
                }
            }

            if (tags.length > 0) {
                blog.tags = tags;
            }

            return blog;

        },

        handlePage: function() {

            var isProductPage = (/^\/?software\/[^\/]+/.test(window.location.pathname) && (ajq(".imkt-navbar__title-wrapper a[href*='/software/']").length > 0 || 
                                ajq(".subnavigation--product.imkt-sticky a[href*='/software/']").length > 0)) ||
                                ((ajq("body.stride nav a.imkt-navbar__link-list-link--current, body.statuspage, body.bitbucket.product nav a.active, body#stride.home").length > 0 || 
                                /bitbucket.org\/product.*/.test(window.location.href) ||
                                /stride.com\/(conferencing|team\-chat|screen\-share)/.test(window.location.href)) && 
                                !/atlassian.com/.test(window.location.origin));

            if (isProductPage) {
                var itemClass;
                if ((/data\-center/.test(window.location.href) && ajq("body.data-center").length > 0) || ajq("body#enterprise.bitbucket").length > 0) {
                    itemClass = {
                        type:               Evergage.ItemType.Tag,
                        tagType:            "ItemClass",
                        _id:                "data-center"
                    };
                    if (Evergage.meta.validateAndTrimItem(itemClass)) {
                        Evergage.viewTag(itemClass);
                    }
                } else if ((/\/(hipchat|crowd)\/pricing/.test(window.location.href))) {
                    itemClass = {
                        type:               Evergage.ItemType.Tag,
                        tagType:            "ItemClass",
                        _id:                "server"
                    };
                    if (Evergage.meta.validateAndTrimItem(itemClass)) {
                        Evergage.viewTag(itemClass);
                    }
                } else {
                    var product = Evergage.meta.detectProduct();
                    if (Evergage.meta.validateAndTrimItem(product)) {
                        Evergage.viewItem(product);
                    }
                }
                Evergage.safeClick(".imkt-tab a[data-tab-name], body.bitbucket.pricing .tab-navigation a[data-name]", function (e) {
                    var target = ajq(e.target);
                    var clickedDeploymentLevel = "";
                    var clickedDeploymentTab = target.attr("data-tab-name") || (target.attr("data-additional-content") || "").replace("section.adg-", "");
                    if (clickedDeploymentTab) {
                        var deploymentLevel = Evergage.meta.detectDeploymentLevel(clickedDeploymentTab);
                        if (deploymentLevel) {
                            itemClass = {
                                type:               Evergage.ItemType.Tag,
                                tagType:            "ItemClass",
                                _id:                deploymentLevel
                            };
                            if (Evergage.meta.validateAndTrimItem(itemClass)) {
                                Evergage.viewTag(itemClass);
                            }
                        }
                    }
                });
                Evergage.safeClick("body", function (e) {
                    if (ajq(e.target).closest("a[href*='/try']").length > 0 ||
                        ajq(e.target).closest("a[href*='/download']#downloads-button").length > 0 ||
                        ajq(e.target).closest("a[href*='/download'].component__link").length > 0 ||
                        ajq(e.target).closest("a[href*='/download']:contains('Try it free')").length > 0 ||
                        ajq(e.target).closest("a[href*='/download'].price-block--cta").length > 0 ||
                        ajq(e.target).closest("a[href*='/download'].button.standard:contains('free')").length > 0 ||
                        ajq(e.target).closest("a.button:contains('Try it free'):not([href*='/download']):not([href*='/try']):not([href*='/purchase'])").length > 0 ||
                        ajq(e.target).closest("a[href*='/download'][href*=sourcetree]").length > 0 ||
                        ajq(e.target).closest(".download-links a").length > 0 ||
                        ajq(e.target).closest(".server a[href*='/download']").length > 0) {
                        if (product && product._id && Evergage.meta.validateAndTrimItem(product)) {
                            Evergage.favorite(product);
                            Evergage.viewItem(product);
                        }
                    } else {
                        return;
                    }
                });
            } else if (/^\/?software\/[^\/]+/.test(window.location.pathname) && ajq("#try a.button[href*='product=']").length > 0) {
                Evergage.safeClick("body", function (e) {
                    if (ajq(e.target).closest("#try a.button[href*='product=']").length > 0) {
                        var products = Evergage.util.getParameterByName("product", ajq(e.target).prop('href'));
                        if (products && products.length > 0) {
                            var productList = products.split(",");
                            for (var i = 0; i < productList.length; i++) {
                                var productId = ajq.trim(productList[i]).split(".")[0];
                                var baseProduct = Evergage.meta.partialMatchProduct(productId);
                                if (baseProduct) {
                                    var product = {
                                        type:               Evergage.ItemType.Product,
                                        _id:                baseProduct,
                                        price:              1
                                    }
                                    var lineItem = {
                                        item:               product,
                                        quantity:           1
                                    };
                                    if (Evergage.meta.validateLineItem(lineItem)) {
                                        Evergage.addToCart(lineItem);
                                    }
                                }
                            }
                        }
                    } else {
                        return;
                    }
                });
           } else if ((/^\/?blog\/[^\/]+/.test(window.location.pathname) && ajq("article.post").length > 0) ||
            (/^blog.bitbucket.org/.test(window.location.host) && Evergage.util.getOpenGraphContent('type') === 'article')) {

                var blog = Evergage.meta.detectBlog();
                if (Evergage.meta.validateAndTrimItem(blog)) {
                    Evergage.viewItem(blog);
                }

            } else if ((ajq(".it-unplugged .article-body").length > 0 && /^\/?it-unplugged\/[^\/]+/.test(window.location.pathname)) ||
                ajq(".exported--content-hub--article .regular-article-copy").length > 0) {

                var article = Evergage.meta.detectArticle();
                if (Evergage.meta.validateAndTrimItem(article)) {
                    Evergage.viewItem(article);
                }
            }

            if (!isProductPage && !/^\/?software\/[^\/]+/.test(window.location.pathname) ) {
                Evergage.safeClick("a:contains('Try')", function (e) {
                    Evergage.trackAction("Clicked Try It");
                });
            }
            if (ajq("body.stride").length > 0 && ajq("a.cta[href*='signup.stride']").length > 0) {
                Evergage.safeClick("body", function (e) {
                    if (ajq(e.target).closest("a.cta[href*='signup.stride']").length > 0) {
                        var product = {
                            type:               Evergage.ItemType.Product,
                            _id:                "stride",
                            price:              1
                        }
                        var lineItem = {
                            item:               product,
                            quantity:           1
                        };
                        if (Evergage.meta.validateLineItem(lineItem)) {
                            Evergage.addToCart(lineItem);
                        }
                    } else {
                        return;
                    }
                });
            }
            
            if (ajq("body.bitbucket").length > 0 && ajq("a[href*='signup']").length > 0) {
                Evergage.safeClick("body", function (e) {
                    if (ajq(e.target).closest("a[href*='signup']").length > 0) {
                        var product = {
                            type:               Evergage.ItemType.Product,
                            _id:                "bitbucket",
                            price:              1
                        }
                        var lineItem = {
                            item:               product,
                            quantity:           1
                        };
                        if (Evergage.meta.validateLineItem(lineItem)) {
                            Evergage.addToCart(lineItem);
                        }
                    } else {
                        return;
                    }
                });
            }
        }

    };
})();

if (ajq("html[lang=en], html[lang='en-US']").length > 0) {
    Evergage.meta.handlePage();
}
    } catch (e) {
        if (typeof window.Evergage === "object" && typeof window.Evergage.getVersion === "function" && window.Evergage.getVersion() >= 5) {
            Evergage.sendException(e, "siteWideJavascript");
        }
    }
