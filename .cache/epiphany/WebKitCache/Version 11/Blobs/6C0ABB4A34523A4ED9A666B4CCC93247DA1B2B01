        /* global window, document, console */
/* global extension, optimizely */
var utils = optimizely.get('utils');

var getCookie = function getCookie(name) {
  var value = "; " + document.cookie;
  var parts = value.split("; " + name + "=");
  if (parts.length === 2) return parts.pop().split(";").shift();
};

var cookie = extension.cookie.replace(/[^\w-]/g, ''); //simplify the provided string

var visitor = window.optimizely.get('visitor');
var languages = [{
  english: 'English',
  native: 'English',
  domain: 'atlassian.com',
  regex: /^en(-[a-z]+)?$/i,
  test: ['en', 'en-us', 'en-gb', 'en-au', 'en-ca', 'en-nz', 'en-ie', 'en-za', 'en-jm', 'en-bz']
}, {
  english: 'Spanish',
  native: 'Español',
  domain: 'es.atlassian.com',
  regex: /^es(-[a-z]+)?$/i,
  test: ['es', 'es-mx', 'es-gt', 'es-cr', 'es-pa', 'es-do', 'es-ve', 'es-co', 'es-pe', 'es-ar', 'es-ec', 'es-cl', 'es-uy', 'es-py', 'es-bo', 'es-sv', 'es-hn', 'es-ni', 'es-pr']
}, {
  english: 'German',
  native: 'Deutsch',
  domain: 'de.atlassian.com',
  regex: /^de(-[a-z]+)?$/i,
  test: ['de', 'de-ch', 'de-at', 'de-lu', 'de-li']
}, {
  english: 'French',
  native: 'Français',
  domain: 'fr.atlassian.com',
  regex: /^fr(-[a-z]+)?$/i,
  test: ['fr', 'fr-be', 'fr-ca', 'fr-ch', 'fr-lu']
}, {
  english: 'Japanese',
  native: '日本語',
  domain: 'ja.atlassian.com',
  regex: /^ja$/i,
  test: ['ja']
}, {
  english: 'Portuguese',
  native: 'Português',
  domain: 'br.atlassian.com',
  regex: /^pt-br$/i,
  test: ['pt-br']
}, {
  english: 'Russian',
  native: 'русский',
  domain: 'ru.atlassian.com',
  regex: /^ru(-[a-z]+)?$/i,
  test: ['ru', 'ru-mo']
}, {
  english: 'Korean',
  native: '한국어',
  domain: 'ko.atlassian.com',
  regex: /^ko$/i,
  test: ['ko']
}];

if (document.cookie.indexOf('wacdev') !== -1) {
  var tests = languages[Math.floor(Math.random() * languages.length)].test;
  visitor.language = tests[Math.floor(Math.random() * tests.length)];
  console.log('Testing with language code:', visitor.language);
}

var renderVars = {
  extension: {
    '$id': extension.$id,
    '$instance': extension.$instance
  },
  listItems: '',
  userLanguage: null,
  viewingLanguage: null,
  switchUrl: null
};

var links = [];
for (var i in languages) {
  var lang = languages[i];
  var _$link = $("<a href=\"" + window.location.href + "\" class=\"lang-nav\">" + lang.native + "</a>").prop('host', lang.domain);

  if (lang.regex.test(visitor.language)) {
    renderVars.userLanguage = lang;
    renderVars.switchUrl = _$link.prop('href');
  }

  if (window.location.host === lang.domain || window.location.host.indexOf('www') !== -1 && lang.english === 'English') {
    links.push("<span class=\"lang-nav\">" + lang.native + "</span>");
    renderVars.viewingLanguage = lang;
  } else {
    links.push(_$link.prop('outerHTML'));
  }
}
renderVars.listItems = '<li>' + links.join('</li><li>') + '</li>';

var $rendered = $(extension.$render(renderVars));

window.optimizely = window.optimizely || [];
window.optimizely.push({
  "type": "user",
  "attributes": {
    "user_language": renderVars.userLanguage ? renderVars.userLanguage.english : 'Other'
  }
});

/**
 * Desktop Navigation:
 */

if (renderVars.userLanguage !== renderVars.viewingLanguage && (!getCookie(cookie) || window.location.href.indexOf('optimizely_local') !== -1)) {
  var $desktop = $rendered.filter(".desktop-only"),
      $page1 = $desktop.find('.page1'),
      $page2 = $desktop.find('.page2').hide(); //.addClass('hide');

  //Hides the butterbar, and adds the cookie to prevent it from reopening.
  $desktop.find('.close').on('click touch', function () {
    window.optimizely.push({
      type: "event",
      eventName: "language_banner_close_banner"
    });

    $desktop.removeClass('show');
    document.cookie = cookie + '=0;domain=atlassian.com;path=/';
  });

  $desktop.find('.switch-langs').on('click touch', function () {
    window.optimizely.push({
      type: "event",
      eventName: "language_banner_switch_to_recommended_language"
    });
  });

  $desktop.find('.all-langs').on('click touch', function () {
    // $page1.addClass('hide');
    // $page2.removeClass('hide');
    window.jQuery($page1).slideUp();
    window.jQuery($page2).slideDown();
    window.optimizely.push({
      type: "event",
      eventName: "language_banner_viewed_all_languages"
    });
  });

  $desktop.find('.lang-nav').on('click touch', function () {
    window.optimizely.push({
      type: "event",
      eventName: "language_banner_switch_language_via_all_language"
    });
  });

  $desktop.find('.back').on('click touch', function () {
    // $page2.addClass('hide');
    // $page1.removeClass('hide');
    window.jQuery($page2).slideUp();
    window.jQuery($page1).slideDown();
  });

  utils.waitForElement('body').then(function () {
    $(document.body).prepend($desktop);
    setTimeout(function () {
      $desktop.addClass('show');
    }, 100);
  });
}

/**
 * Mobile Navigation:
 */

// let $mobile = $('<li class="mobile-only header_language_alert open">');
// let $link = $(`<a class="cms-link cms-link--text">Change language</a>`).appendTo($mobile);
var $mobile = $rendered.filter(".mobile-only");
var $link = $mobile.find('.cms-link');
var $ul = $mobile.find('.lang-list').hide();

// let $ul = $(`<ul class="lang-list">${languageListItems}</ul>`).appendTo($mobile);
$link.on('click touch', function (e) {
  $link.toggleClass('open');
  window.jQuery($ul).slideToggle(function () {
    window.jQuery(window).trigger('resize'); //resize the content afterwards
  });
  if ($link.hasClass('open')){
    window.optimizely.push({
      type: "event",
      eventName: "language_banner_viewed_all_languages"
    });
  }
  e.preventDefault();
  e.stopPropagation();
});

$mobile.find('.lang-nav').on('click touch', function () {
  window.optimizely.push({
    type: "event",
    eventName: "language_banner_switch_language_via_all_language"
  });
});

utils.waitForElement('#topnav .nav-wrapper > ul').then(function (el) {
  $(el).append($mobile);

  $('.menu-toggle').on('click touch', function () {
    //cause the navigation to resize it's elements
    window.jQuery(window).trigger('resize');
  });
});
