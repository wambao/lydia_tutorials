  if(window.location.search.indexOf("tab=self-hosted") !== -1) {
    activate();
  }
  
  window.optimizely.get("utils")
    .waitForElement(".imkt-tab-list .imkt-tab:nth-child(2) a.active")
  	.then(activate);
  
  //This gets run on pages outside of the url targeting...
  if (options.isActive){
    /* * * * * * * * * *
     * Config variables
     * * * * * * * * * */
    var pagePrefix = 'confluence_pricing';
    var thresholds = { //# Users
      cloud: 500,
      server: 500
    };
    /* * * * * * * * * */

    var utils = optimizely.get('utils'), $ = optimizely.get('jquery');
    //Cloud:
    utils.waitForElement('#usersSelectDropdown').then(function(el){
      var $el = $(el);
      $el.change(function(e){
        var higher = parseInt($el.val()) >= thresholds.cloud ? "high" : "normal";
        optimizely.push({
          type: "event",
          eventName: pagePrefix+"_cloud_selected_"+higher+"_value"
        });
      });
    });

    //Server:
    utils.waitForElement('#usersSelectDropdown1').then(function(el){
      var $el = $(el);
      $el.change(function(e){
        var higher = parseInt($el.val()) >= thresholds.server ? "high" : "normal";
        optimizely.push({
          type: "event",
          eventName: pagePrefix+"_server_selected_"+higher+"_value"
        });
      });
    });

    //Data Center:
    utils.waitForElement('#usersSelectDropdown2').then(function(el){
      var $el = $(el);
      $el.change(function(e){
        //Any value is high value
        optimizely.push({
          type: "event",
          eventName: pagePrefix+"_data_center_selected_high_value"
        });
      });
    });
  }
