    var init = function() {
        pollForElement('#start-trial .component > h1');
        pollForElement('form button.button > span.submit--initial-text');
    },

    pollForElement = function(selector) {
        var $selector = $(selector);
        if ($selector.length > 0) {
          switch(selector) {
              case '#start-trial .component > h1':
                    $selector.text('Built for teams of 10, and teams of 10,000');
                    $selector.parent().parent().addClass('fy18q1_me_37');
                  break;

              case 'form button.button > span.submit--initial-text':
                    $selector.text('Try it free');
                    $selector.parent().addClass('fy18q1_me_37');
                    break;
              default:
          }

        } else {
            setTimeout(function() {
                pollForElement(selector);
            }, 10);
        }
    };

  return {
      'init': init
  };
})();

