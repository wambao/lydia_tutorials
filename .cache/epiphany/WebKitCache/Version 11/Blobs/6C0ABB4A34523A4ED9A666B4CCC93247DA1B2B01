window.optimizely = window.optimizely || [];
function _defineProperty(obj, key, value) { if (key in obj) { Object.defineProperty(obj, key, { value: value, enumerable: true, configurable: true, writable: true }); } else { obj[key] = value; } return obj; }

var PJS = {};
PJS.isDev = /cro(metrics)?-debug|optimizely_local/.test(window.location.search) || localStorage && localStorage.getItem("cro-debug");
PJS.log = PJS.isDev && console && console.info && console.info.bind(console, '[PJS]') || function () {};
PJS.error = function (errorCode, details) {
  if (console && console.error) console.error('PJS Error:', errorCode, '\n', details);
};

try {
  /**
    PJS Error Reporting
    @author Andrew Wessels <andrew.wessels@crometrics.com>
    Last Updated: 3/18/18
  
    Sends an Optimizely event if a PJS error is caught.
  */
  PJS.error = function () {
    var error = PJS.error;
    return function (errorCode, details) {
      errorCode = 'analytics.failure.' + errorCode;
      window.optimizely.push(["trackEvent", errorCode]);
      error(errorCode, details);
    };
  }();
} catch (e) {
  PJS.error('atl-error-reporting', e);
}

try {
  /**
   * Atlassian's growth team (GROW) methods
   * @author Shreyas <shreyas@atlassian.com>
   * @author Andrew Wessels <andrew.wessels@crometrics.com>
   * Last updated: 12/19/17
   */
  window.GROW = window.GROW || {}; //global namespace with for growth team's methods

  window.GROW.getCookie = function (name) {
    var value = "; " + document.cookie;
    var parts = value.split("; " + name + "=");
    if (parts.length === 2) return parts.pop().split(";").shift();
  };

  window.GROW.getDecodedCookie = function (name) {
    return decodeURIComponent(window.GROW.getCookie(name));
  };

  window.GROW.setCookie = function (name, val) {
    //reference: http://stackoverflow.com/a/8733385/1887264
    var expiration_date = new Date();
    var cookie_string = '';
    expiration_date.setFullYear(expiration_date.getFullYear() + 1);
    // Build the set-cookie string:
    cookie_string = name + '=' + val + '; path=/; expires=' + expiration_date.toGMTString();
    // Create/update the cookie:
    document.cookie = cookie_string;
  };

  window.GROW.setDarkFeature = function (key, val) {
    window.GROW.darkFeatures = window.GROW.darkFeatures || {};
    window.GROW.darkFeatures[key] = val;
  };

  window.GROW.getDarkFeature = function (key) {
    return (window.GROW.darkFeatures || {})[key];
  };
} catch (e) {
  PJS.error('atl-growth-team-methods', e);
}

try {
  /**
   * New vs. Returning Visitors Attributes
   * @author Shreyas <shreyas@atlassian.com>
   * @author Andrew Wessels <andrew.wessels@crometrics.com>
   * Last updated: 12/19/17
   * Requires: window.GROW
   *
   * Attributes:
   *   visitorid_anonymousid
   *   visitorid_atlpath
   *   visitorid_userid
   *   visitor_type_onatlassian
   *   visitor_type_onsegment
   *   customer_type_onsegment
   *
   * Record Segment's "New & Returning" segmentation in Optimizely results.
   */
  ;(function () {
    try {
      // record user segment /w Optimizely Attribute API
      var pushUserSegment = function pushUserSegment(segment_name, segment_value) {
        var value = encodeURIComponent(segment_value);
        window.optimizely.push({
          type: "user",
          attributes: _defineProperty({}, segment_name, value)
        });
      };

      var anonId = window.GROW.getDecodedCookie("ajs_anonymous_id"),
          userId = window.GROW.getDecodedCookie("ajs_user_id"),
          atlPath = window.GROW.getDecodedCookie("__atl_path");

      // record raw visitor ids
      pushUserSegment("visitorid_anonymousid", anonId);
      pushUserSegment("visitorid_atlpath", atlPath);
      pushUserSegment("visitorid_userid", userId);

      var newOrReturning = function newOrReturning(value) {
        return value === "undefined" || value === "null" ? "new" : "returning";
      };
      pushUserSegment("visitor_type_onatlassian", newOrReturning(atlPath));
      pushUserSegment("visitor_type_onsegment", newOrReturning(anonId));

      var customerSegment = {
        "undefined": "undefined",
        "null": "not_a_customer"
      }[userId] || "customer";

      pushUserSegment("customer_type_onsegment", customerSegment);
    } catch (e) {
      window.optimizely.push(["trackEvent", "optimizelygoal.error.recording.segmentation.in_projectjs"]);
    }
  })();
} catch (e) {
  PJS.error('atl-segment-attributes', e);
}

try {
  /**
    iframe CSS fix
    @author Chase Marlow <chase.marlow@crometrics.com>
    Date: 1/16/18
    Last Updated: 1/19/18
  */
  ;(function () {
    window.optimizely.push({
      'type': 'addListener',
      'filter': {
        'type': 'lifecycle',
        'name': 'originsSynced'
      },
      'handler': function handler() {
        var utils = window.optimizely.get('utils');

        utils.observeSelector('main+iframe+footer', function () {
          var iframe = document.querySelector('main+iframe');
          iframe.parentNode.insertBefore(iframe, iframe.previousNode);
        }, {
          'timeout': 5000,
          'once': true
        });
      }
    });
  })();
} catch (e) {
  PJS.error('iframe-fix', e);
}

try {
  /**
    Demandbase Gold/Silver Audience Cookie and Attributes
    @author Andrew Wessels <andrew.wessels@crometrics.com>
    Last Updated: 3/19/18
    Requires: window.GROW
  */
  ;(function () {
    //Wait for Optimizely state so that visitor data is available
    function activated() {
      var demandbase = window.optimizely.get('visitor')['vendor.demandbase'];
      var status = demandbase && demandbase.watch_list && demandbase.watch_list.account_status;

      if (status === 'Gold' || status === 'Silver') {
        window.GROW.setCookie('_bxp_crometrics_treatment', 'true');
      }

      var attributes = {
        demandbase_account_type: function demandbase_account_type() {
          return status || null;
        },
        demandbase_forbes_2000: function demandbase_forbes_2000() {
          return (!!demandbase.forbes_2000).toString();
        },
        demandbase_fortune_1000: function demandbase_fortune_1000() {
          return (!!demandbase.fortune_1000).toString();
        },
        demandbase_high_value: function demandbase_high_value() {
          return (demandbase.forbes_2000 || demandbase.fortune_1000 || status === "Gold" || status === "Silver").toString();
        }
      };
      for (var attributeName in attributes) {
        try {
          attributes[attributeName] = attributes[attributeName]();
        } catch (e) {
          attributes[attributeName] = null;
        }
      }

      //Set all demandbase attributes at once
      window.optimizely.push({
        type: "user",
        attributes: attributes
      });
    }

    window.optimizely.push({
      type: "addListener",
      filter: {
        type: "lifecycle",
        name: "activated"
      },
      handler: activated
    });
  })();
} catch (e) {
  PJS.error('atl-demandbase', e);
}
