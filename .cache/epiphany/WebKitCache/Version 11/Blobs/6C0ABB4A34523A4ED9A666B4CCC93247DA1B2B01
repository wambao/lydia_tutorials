/* _optimizely_evaluate=force */

$('html').addClass('bxpsci136');
var EXP = EXP || {};

EXP.atl_tracking = (function() {
    var seg_xid = window.GROW.getCookie('seg_xid'),
        version = 1,

    sendTrack = function(identity, exp_id, var_id, e_name, props) {
        // standardize event name
        e_name = 'grow.expevt.' + exp_id + '.' + e_name;
        sendToOptimizely(e_name);
        sendToGAS(identity, exp_id, var_id, e_name, props);
        sendToSegmentJS(identity, exp_id, var_id, e_name, props);
        sendToSegmentHTTP(identity, exp_id, var_id, e_name, props);
    },
    sendToOptimizely = function(e_name) {
        window.optimizely.push(['trackEvent', 'bxpsci136.' + e_name]);
    },
    sendToSegmentJS = function(identity, exp_id, var_id, e_name, props) {
        var data = {
            'experiment_id': exp_id,
            'variation_index': var_id,
            'user': identity,
            'product': 'wac',
            'page_path': window.location.pathname,
            'page_url': window.location.href,
            'referrer': document.referrer,
            'version': version
        };

        if(props) {
            for (var key in props) {
    			data[key] = props[key];
    		}
        }

        try {
            analytics.track(e_name, data);
        } catch(e) {
            sendToGAS('ERR: ' + e);
        }
    },
    sendToSegmentHTTP = function(identity, exp_id, var_id, e_name, props) {
        var anonId = GROW.getDecodedCookie('ajs_user_id').replace(/"/g, '');

        var data = {
            'anonymousId': anonId,
              'event': e_name,
              'timestamp': new Date(),
              'writeKey': 'kiv6wyh2nw',
              'properties': {
                  'experiment_id': exp_id,
                  'variation_index': var_id,
                  'user': identity,
                  'product': 'wac',
                  'page_path': window.location.pathname,
                  'page_url': window.location.href,
  	              'referrer': document.referrer,
                  'version': version
              }
        };

        for (var key in props) {
            data.properties[key] = props[key];
        }

        jQuery.ajax({
          type: 'POST',
          url: 'https://api.segment.io/v1/track',
          data: JSON.stringify(data),
          contentType: 'application/json'
        })
        .fail(function() {
            sendToOptimizely('segmentHTTP_fail');
            sendToGAS('segmentHTTP_fail');
        });
    },
	sendToGAS = function(identity, exp_id, var_id, e_name, props) {
			props = props || {};

	    var user_id = decodeURIComponent(GROW.getCookie('ajs_user_id')),
	        anon_id = decodeURIComponent(GROW.getCookie('ajs_anonymous_id')),
	        atl_path = decodeURIComponent(GROW.getCookie('__atl_path')),
	        ga = decodeURIComponent(GROW.getCookie('_ga')),
	        sio = decodeURIComponent(GROW.getCookie('_sio')),
	        opt_id = decodeURIComponent(GROW.getCookie('optimizelyEndUserId')),
	        page_path = encodeURIComponent(window.location.href);

	    var eventData = {
            "name": e_name,
	        "server": "veritas",
	        "product": "wac",
	        "user": identity,
	        "serverTime": (new Date()).getTime(),
	        "properties": {
                "path": page_path,
	            "referrer": document.referrer,
	            "exp_id": exp_id,
	            "var_id": var_id,
	            "anon_id": anon_id,
	            "atl_path": atl_path,
	            "ga": ga,
	            "sio": sio,
	            "opt_id": opt_id,
                "seg_xid": seg_xid,
                "version": version
	        }
	    };

		for (var key in props) {
			eventData.properties[key] = props[key];
		}

	    // using page jQuery, because Optimizely $ doesn't have ajax().
	    jQuery.ajax({
            url: "https://analytics.atlassian.com/analytics/event",
            data: JSON.stringify(eventData),
            type: "POST",
            contentType: "application/json"
        });
	};
    return {
        'sendTrack': sendTrack
    };
})();

EXP.bxpsci136 = (function() {
    var identity,
        seg_xid = window.GROW.getCookie('seg_xid'),
        exp_id = '11200131232',
        var_id = optimizely.get('state').getVariationMap()[exp_id].id,
        // var_id = 1,

    sendTrack = function(e_name, props) {
        EXP.atl_tracking.sendTrack(identity, exp_id, var_id, e_name, props);
    },

    init = function() {
        identity = window.GROW.getCookie('bxpsci136_identity');
        if (typeof identity === 'undefined' || identity === 'undefined') {
            // no id
            identity = generateUUID();
            window.GROW.setCookie('bxpsci136_identity', identity);
        }

        $(document).ready(function(){
            analytics.ready(function() {
                sendTrack('pageview.' + location.pathname.replace(/\//g,'_'));
            });
        });

        var utils = window['optimizely'].get('utils');

        // For www.atlassian.com/try
        if(location.pathname === '/try') {
            utils.waitForElement('.global-nav--wac__button--primary').then(function(cta) {
                $(cta).click(function(){
                    sendTrack('bxpsci136_wacTry_cta_clicked');
                });
            });

            // clicked JSW try cloud
            utils.waitForElement('section:nth-of-type(2) div:nth-of-type(2) .green-hollow').then(function(cloud) {
                $(cloud).click(function(){
                    sendTrack('bxpsci136_wacTry_jsw_cloud_clicked');
                });
            });

            // clicked Confluence try cloud
            utils.waitForElement('.collaborate div:nth-of-type(2) .green-hollow').then(function(server) {
                $(server).click(function(){
                    sendTrack('bxpsci136_wacTry_conf_cloud_clicked');
                });
            });
        }

        // For www.atlassian.com/software/jira/try
        if(location.pathname === '/software/jira/try') {
            utils.waitForElement('div:nth-of-type(2) > .standard').then(function(cta) {
                $(cta).click(function(){
                    sendTrack('bxpsci136_jiraTry_cta1');
                });
            });

            utils.waitForElement('.column--featured > .standard').then(function(cta) {
                $(cta).click(function(){
                    sendTrack('bxpsci136_jiraTry_cta2');
                });
            });

            utils.waitForElement('div:nth-of-type(4) > .standard').then(function(cta) {
                $(cta).click(function(){
                    sendTrack('bxpsci136_jiraTry_cta3');
                });
            });
        }

        // For www.atlassian.com/software/confluence/try
        if(location.pathname === '/software/confluence/try') {
            utils.waitForElement('div:nth-of-type(2) > .standard').then(function(cta) {
                $(cta).click(function(){
                    sendTrack('bxpsci136_confTry_cta1');
                });
            });

            utils.waitForElement('.column--featured > .standard').then(function(cta) {
                $(cta).click(function(){
                    sendTrack('bxpsci136_confTry_cta2');
                });
            });

            utils.waitForElement('div:nth-of-type(4) > .standard').then(function(cta) {
                $(cta).click(function(){
                    sendTrack('bxpsci136_confTry_cta3');
                });
            });
        }
    },
    generateUUID = function() {
		var d = new Date().getTime();
		var uuid = 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {
		    var r = (d + Math.random() * 16) % 16 | 0;
		    d = Math.floor(d / 16);
		    return (c == 'x' ? r : (r & 0x3 | 0x8)).toString(16);
		});
		return uuid;
	};
    return {
        'init': init
    };
})();

EXP.bxpsci136.init();
/* _optimizely_evaluate=normal */
