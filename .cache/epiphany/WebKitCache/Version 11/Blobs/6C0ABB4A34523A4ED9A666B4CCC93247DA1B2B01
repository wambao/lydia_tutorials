  activate();
  if (options.isActive){
  	var $ = window.optimizely.get('jquery');

    $(window).on('load', function(){
      // Wait for the window to load so the iframe should
      //   have it's initial content.

    	$('#enterprise_contact_form').on('load', function(){
        // If the iframe starts loading it's because the form was submitted.
        window.optimizely = window.optimizely || [];
        window.optimizely.push({
          type: "event",
          eventName: "enterprise_contact_form_submit"
        });
      });
    });

    //<Surveygizmo Event Logic>
    console.log('Surveygizmo event logic ready. v0.2');
    var prefillProduct = window.location.hash.substr(1).toLowerCase();
    var prefillOptions = ['jira','jsd','confluence','bitbucket','hipchat','crowd','premier-support','tam','stack'];
    var prefillDone = false;

    window.optimizely = window.optimizely || [];
    window.addEventListener('message', function(e){
      if (e.data && e.origin === "https://www.surveygizmo.com"){
        //Attempt to prefill the product checkbox selection after surveygizmo initializes and sends it's first event.
        if (!prefillDone && prefillOptions.indexOf(prefillProduct) !== -1){
          document.getElementById('enterprise_contact_form').contentWindow.postMessage({prefill: prefillProduct}, "https://www.surveygizmo.com");
          window.optimizely.push({
            type: "user",
            attributes: {
              "cro-enterprise-contact": prefillProduct
            }
          });
        }
        prefillDone = true;

        if (e.data.event){
          console.log('form event:', e.data);
          //This one can be deprecated once PER-8 is turned off.
          window.optimizely.push({
            type: "event",
            eventName: "per-8-form-engagement"
          });
          window.optimizely.push({
            type: "event",
            eventName: "enterprise-contact-form-engagement"
          });
        }
      }
    });
    //</Surveygizmo Event Logic>

  }
}
