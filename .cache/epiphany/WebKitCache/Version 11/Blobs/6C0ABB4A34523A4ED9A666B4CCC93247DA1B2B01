widget._styleTag.innerHTML = '.ext-butterbar-spacer {  height: 0;  transition: height 0.5s;  will-change: height; }.ext-butterbar {  background-color: #c7e3f3;  margin: 0;  text-align: center;  width: 100%;  overflow: hidden;  position: absolute; }  .ext-butterbar .slider {    height: 50px;    transition: margin-top 0.5s;    margin-top: -50px;    will-change: margin-top; }  .ext-butterbar .content {    font-family: Helvetica, arial, sans-serif;    margin: 0;    font-size: 16px;    line-height: 30px;    color: #4a4a4a;    font-weight: normal;    padding: 10px;    display: inline-block; }    .ext-butterbar .content a {      color: #205281; }      .ext-butterbar .content a:hover {        text-decoration: underline; }  .ext-butterbar .close {    float: right;    font-size: 22px;    line-height: 22px;    color: #3572b1;    margin-top: 14px;    padding-right: 50px;    cursor: pointer;    opacity: .75;    text-shadow: 0 1px 0 #fff; }    .ext-butterbar .close:hover {      opacity: 1; }    @media only screen and (max-width: 1200px) {      .ext-butterbar .close {        padding-right: 10px; } }header.banner > .ext-butterbar {  position: absolute; }.ext-butterbar-wrapper.ext-sticky.ext-blog .ext-butterbar {  position: fixed;  z-index: 1; }.ext-butterbar-wrapper.ext-sticky.ext-stuck.ext-main1 .ext-butterbar {  position: fixed;  top: 0;  z-index: 10; }.ext-butterbar-wrapper.ext-sticky.ext-stuck.ext-main2 .ext-butterbar {  position: fixed;  top: 60px;  z-index: 10; }.ext-butterbar-wrapper.ext-show .ext-butterbar .slider {  margin-top: 0;  transition: margin-top 1s; }.ext-butterbar-wrapper.ext-show .ext-butterbar-spacer {  height: 50px;  transition: height 1s; }@media only screen and (max-width: 959px) {  .ext-butterbar-wrapper {    display: none; } }';
document.getElementsByTagName('head')[0].appendChild(widget._styleTag);
        /* global window, document, console */
/* global optimizely, extension */
var getCookie = function getCookie(name) {
  var value = "; " + document.cookie;
  var parts = value.split("; " + name + "=");
  if (parts.length === 2) return parts.pop().split(";").shift();
};

var cookie = extension.cookie.replace(/[^\w-]/g, ''); //simplify the provided string

console.log("[cro] extension.sticky", extension.sticky);
var sticky = extension.sticky === 'sticky';
$(function () {
  if (!document.getElementById('wat-promotion-banner') && !getCookie(cookie)) {
    var $extension = $(extension.$html).addClass('ext-' + (sticky ? 'sticky' : 'static'));

    /*
    * Show message under sub-nav if sub-nav exists otherwise show directly under header (main nav)
    * If there is anything in between header and sub-nav, show under header
    */
    if (window.location.pathname.substr(0, 5).toLowerCase() === '/blog') {
      $extension.addClass('ext-blog');
      // $extension.insertAfter('.header-wrap > header > .container')//.wrap('<div style="overflow: hidden">');
      $extension.insertAfter('.header-wrap');
    } else {
      var nav = document.querySelector('.imkt-sticky-wrapper');
      console.log("[cro] nav", nav);
      if (nav && nav.parentNode.nodeName === 'MAIN') {
        $extension.addClass('ext-main2');
        $(nav).after($extension);
      } else {
        $extension.addClass('ext-main1');
        $('body > header').after($extension);
      }
    }

    var $document = $(document);
    var targetTop = $extension.hasClass('ext-main2') ? 60 : 0;
    var onScroll = function onScroll() {
      var bounds = $extension[0].getBoundingClientRect();
      if (bounds.top < targetTop) {
        $extension.addClass('ext-stuck');
      } else {
        $extension.removeClass('ext-stuck');
      }
    };

    var show = function show() {
      if (sticky) $document.on('scroll', onScroll);

      $extension.addClass('ext-show');
    };

    //Hides the butterbar, and adds the cookie to prevent it from reopening.
    var finished = function finished() {
      $document.off('scroll', onScroll);
      $extension.removeClass('ext-show');
      document.cookie = cookie + '=0;domain=atlassian.com;path=/';
    };

    $extension.find('.close').on('click touch', function () {
      finished();
    });

    //Automatically fire off this custom event for CTA clicks:
    $extension.find('#wat-promotion').on('click touch', function () {
      optimizely.push({
        type: "event",
        eventName: "butterbar_cta_click"
      });
      finished(); //Also close the butterbar, and prevent it from reopening.
    });

    setTimeout(function () {
      show();
    }, 100);
  }
});
      },
      hideFn: function(event) {
        var $ = window.optimizely.get('jquery');
        var widget = event.data.config;
        widget.$id = "butterbar";
        widget.$instance = event.data.id;
        widget.$fieldDefaults = [{"name":"cookie","default_value":"promo123"},{"name":"text","default_value":"Simple paragraph <s>text</s>, leading to the "},{"name":"link_text","default_value":"Promotion"},{"name":"link_url","default_value":"https://www.atlassian.com/"},{"name":"sticky","default_value":"sticky"}];
        (function(widg) {
  var i = 0;
  var field;
  for (; i < widg.$fieldDefaults.length; ++i) {
    field = widg.$fieldDefaults[i];
    if (!widg.hasOwnProperty(field.name)) {
      widg[field.name] = field.default_value;
    }
  }
  })(widget);
        var extension = widget;
        widget._styleTag = document.getElementById('widget-css-butterbar');
if (widget._styleTag) widget._styleTag.parentNode.removeChild(widget._styleTag);
        var extensionHtml = document.getElementById('wat-promotion-banner');
if (extensionHtml) extensionHtml.remove();

      },
    });

